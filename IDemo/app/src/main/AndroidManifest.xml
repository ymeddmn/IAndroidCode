<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demo.idemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".anim.AnimListActivity" />
        <activity android:name=".anim.ObjectAnimatorActivity" />
        <activity android:name=".anim.KeyFrameAnimatorActivity" />
        <activity android:name=".anim.CircleImageActivity" />
        <activity android:name=".anim.AlphaAnimationActivity" />
        <activity android:name=".anim.TranslationAnimationActivity" />
        <activity android:name=".anim.ScaleAnimationActivity" />
        <activity android:name=".anim.RotationAnimationActivity" />
        <activity android:name=".anim.ValueAnimatorActivity" />
        <activity android:name=".anim.ImageTurnActivity" />
        <activity
            android:name=".anim.ImageShowActivity"
            android:theme="@style/NoTitleTranslucentTheme" />
        <activity
            android:name=".anim.ImageShowActivity1"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleTranslucentTheme" />
        <activity
        android:name=".anim.ShowImageActivity2"
        android:screenOrientation="portrait"
        android:theme="@style/NoTitleTranslucentTheme" />

        <activity
            android:name=".anim.ShowImageActivity3"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleTranslucentTheme" />
    </application>

</manifest>